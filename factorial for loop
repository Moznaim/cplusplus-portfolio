#include <iostream>
using namespace std;
int main()
{
int number, factorial=1;
  
    cout << "Enter a number: ";
    cin >> number;
  
  if (number <= 9) {
  for (int i = 1; i<=number; i++) {
   
    factorial= factorial*i;
    
    }
    cout << "The factorial of "<< number << ": " << factorial;
    }
  else {
    cout << "Number is too high";
  }
  return 0;
  }



