#include <iostream>
using namespace std;
int main() {
  
int sum = 0;
int i; //initialization 

  cout << "Please input an integer: ";
  cin >> i;
  
  while (i <= 9) // test expression 
    {
      cout << i << endl;
      sum = i + sum;
      i++; //update expression
    }
  cout << "The Sum is: " << sum; 


  
}
